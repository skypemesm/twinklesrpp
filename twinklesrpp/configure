            { $as_echo "$as_me:${as_lineno-$LINENO}: result: $QT_LIBS" >&5
$as_echo "$QT_LIBS" >&6; }
        else
            QT_CFLAGS=""
            QT_LIBS=""
            ## If we have a custom action on failure, don't print errors, but
            ## do set a variable so people can do so.
            QT_PKG_ERRORS=`$PKG_CONFIG --errors-to-stdout --print-errors "qt-mt >= 3.3.0 qt-mt < 4.0"`
            echo $QT_PKG_ERRORS
        fi



     else
        echo "*** Your version of pkg-config is too old. You need version $PKG_CONFIG_MIN_VERSION or newer."
        echo "*** See http://www.freedesktop.org/software/pkgconfig"
     fi
  fi

  if test $succeeded = yes; then
     :
  else
     as_fn_error "Library requirements (qt-mt >= 3.3.0 qt-mt < 4.0) not met; consider adjusting the PKG_CONFIG_PATH environment variable if your libraries are in a nonstandard prefix so pkg-config can find them." "$LINENO" 5
  fi

fi

# Restore the original value of PKG_CONFIG_PATH
PKG_CONFIG_PATH=$OLD_PKG_CONFIG_PATH
export PKG_CONFIG_PATH

